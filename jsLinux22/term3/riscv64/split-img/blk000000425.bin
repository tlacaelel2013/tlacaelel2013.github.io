    assert(rc.ptr is null);
    rc = rebindable(cs3);
    rc = rebindable(si);
    assert(rc.ptr !is null);

    ri.destroy;
    assert(ri.ptr is null);
    ri = rebindable(si);
    assert(ri.ptr !is null);
}

